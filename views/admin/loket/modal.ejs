<!-- Modal -->
<div class="modal fade" id="modelId" tabindex="-1" role="dialog" aria-labelledby="modelTitleId" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">Tambah Loket</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="container-fluid">
                    <div class="alert alert-warning err-message" role="alert">
                        <strong>Error</strong>
                    </div>
                    <form action="#" id="userForm" method="post">
                        <div class="row">
                            <div class="col-6">
                                <div class="form-group">
                                    <label class="font-weight-bold" for="no_loket">No Loket</label>
                                    <input type="number" class="form-control" name="no_loket" id="no_loket" placeholder="Ex: 3">
                                    <div class="invalid-feedback">
                                        No loket tidak valid!
                                    </div>
                                </div>
                            </div>
                            <div class="col-6">
                                <div class="form-group">
                                    <label class="font-weight-bold" for="layanan">Layanan</label>
                                      <select class="form-control" name="layanan" id="layanan">
                                          <option value="" selected>Pilih Layanan</option>
                                          <% for(const layanan of layanans){ %>
                                          <option value="<%= layanan.id %>"><%= layanan.nama %></option>
                                          <% } %>
                                      </select>
                                    <div class="invalid-feedback">
                                        Silahkan pilih layanan terlebih dahulu!
                                    </div> 
                                </div>
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="font-weight-bold" for="user">Staff</label>
                              <select class="form-control" name="user" id="user">
                                  <option value="" selected>Pilih Staff (Petugas)</option>
                                  <% for(const staff of staffs){ %>
                                  <option value="<%= staff.id %>"><%= staff.name %></option>
                                  <% } %>
                              </select>
                            <div class="invalid-feedback">
                                Silahkan pilih Staff (Petugas) terlebih dahulu!
                            </div> 
                        </div>

                        <div class="d-flex justify-content-end">
                            <button
                                class="btn btn-primary btn-block waves-effect waves-light d-flex justify-content-center align-items-center"
                                type="submit" style="width:fit-content;">
                                <div class="loader-wrapper hide">
                                    <div class="loader"></div>
                                </div>
                                <span class="mx-2">Submit</span>
                            </button>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>


<!-- Modal -->
<div class="modal fade" id="editModal" tabindex="-1" role="dialog" aria-labelledby="modelTitleId" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">Edit Loket</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="container-fluid">
                    <div class="alert alert-warning err-message" role="alert">
                        <strong>Error</strong>
                    </div>
                    <div class="loader-modal"></div>
                    <form class="hide" action="#" id="userEditForm" method="post" data-id="0">
                        <input type="hidden" id="editId" value="">
                        <div class="row">
                            <div class="col-6">
                                <div class="form-group">
                                    <label class="font-weight-bold" for="no_loket_edit">No Loket</label>
